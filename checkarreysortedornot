#include<bits/stdc++.h>
using namespace std;
bool Sorted(vector<int>&arr,int i){

    if(i==arr.size()-1){
        return true;
    }
    else {
        return false;
    }

    if(arr[i]<arr[i+1]){
       return Sorted(arr,i+1);
}
    }
    

    
int main()
{
  vector<int>arr = {3};
  bool ans = Sorted(arr,0);
  cout<<ans<<endl;
  
  return 0; 
}
